<!DOCTYPE html>
<html>

<head>
    <title>Online C# Compiler</title>
    <link rel="shortcut icon" href="~/assets/icons/logo.png" />
    <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
    <link rel="stylesheet" type="text/css" href="/fonts/font.css">
    <script src="/js/Programizcomnew.min.js"></script>

    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=0" />
    <link rel="canonical" href="https://www.programiz.com/csharp-programming/online-compiler/" />
    <meta name="description" content="Write and run your C# code using our online compiler. Enjoy additional features like code sharing, dark mode, and support for multiple programming languages." />

    <!-- Meta Tags for Social Media -->
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://www.programiz.com/csharp-programming/online-compiler/" />
    <meta property="og:title" content="Online C# Compiler" />
    <meta property="og:description" content="Write and run your C# code using our online compiler. Enjoy additional features like code sharing, dark mode, and support for multiple programming languages." />
    <meta property="og:image" content="https://programiz.com/online-compiler/assets/logos/compiler_logo.png" />
    <meta name="twitter:card" content="summary">
    <meta property="twitter:url" content="https://www.programiz.com/csharp-programming/online-compiler/" />
    <meta name="twitter:title" content="Online C# Compiler">
    <meta name="twitter:description" content="Write and run your C# code using our online compiler. Enjoy additional features like code sharing, dark mode, and support for multiple programming languages." />
    <meta name="twitter:image" content="https://programiz.com/online-compiler/assets/logos/compiler_logo.png" />

    <script src="https://cdnjs.cloudflare.com/ajax/libs/ace/1.4.11/ace.js"></script>
    <script src="https://cmp.uniconsent.com/v2/stub.min.js"></script>
    <script async src='https://cmp.uniconsent.com/v2/a8d3ae4937/cmp.js'></script>
    <link rel="stylesheet" href="https://cdn.playground-v2.programiz.com/playground.css?v=1fsaf" />
    <link rel="stylesheet" type="text/css" href="/css/site.css">

</head>

<body>
    <div class="container">
        <div class="spinner">
            <div class="loader"><span>{</span><span>}</span></div>
        </div>

        <div class="wrapper">
            <!-- Header -->
            <partial name="_Header" />

            <!-- SideBar -->
            @{
                var hideSidebar = ViewData["HideSidebar"] as bool? ?? false;
            }

            @if (!hideSidebar)
            {
                <partial name="_SideBar" />
            }

            <!-- Render Body -->
            @RenderBody()

            <!-- Terminal -->
            @{
                var hideTerminal = ViewData["HideTerminal"] as bool? ?? false;
            }

            @if (!hideTerminal)
            {
                <partial name="_Terminal" />
            }

        </div>
    </div>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            document.querySelector('.spinner').style.display = 'none'; 
            document.querySelector('.wrapper').style.display = 'block'; 
        });
    </script>
    <!-- Other JavaScript files -->
    <script defer src="~/js/pythonCompilerHandler.js"></script>
    <script defer src="~/js/shareCode.js"></script>
    <script defer src="~/js/compileHistory.js"></script>
    <script defer src="~/js/uploadFile.js"></script>
    <script defer src="~/js/changeSideBar.js"></script>
    <script defer src="~/js/hideSpinner.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/ace/1.4.11/ace.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/ace/1.4.11/ext-language_tools.js"></script>
    <script defer src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.4.5/socket.io.min.js"></script>
    <script defer src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js"></script>
    <script defer src="https://cdnjs.cloudflare.com/ajax/libs/mousetrap/1.4.6/mousetrap.min.js"></script>
    <script defer src="https://cdn.playground-v2.programiz.com/build/final.js?v=resize-fixv3"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

</body>
</html>